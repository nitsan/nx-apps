        "options": {
          "outputPath": "packages/my-package1/dist",
          "main": "packages/my-package1/src/index.ts",
          "tsConfig": "packages/my-package1/tsconfig.lib.json",
          "format": [
            "esm"
          ],
          "declarationRootDir": "packages/my-package1/src"
        }
      }
    }
  },
